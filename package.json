    "sinon":"1.17.6",
    "sinon-chai":"2.8.0",
    "eslint": "3.11.0",
    "eslint-config-standard": "6.2.1",
    "eslint-config-semistandard": "7.0.0",
    "eslint-plugin-standard": "2.0.1",
    "eslint-plugin-promise": "3.4.0",
    "eslint-plugin-jsdoc": "3.0.0",
    "supertest":"2.0.1",

