{
  "name": "ravel",
  "version": "0.18.3",
  "author": "Sean McIntyre <s.mcintyre@xverba.ca>",
  "description": "Ravel Rapid Application Development Framework",
  "engines": {
    "node": ">=6.0.0"
  },
  "keywords": [
    "koa",
    "web",
    "app",
    "http",
    "application",
    "architecture",
    "framework",
    "middleware",
    "generators",
    "es6",
